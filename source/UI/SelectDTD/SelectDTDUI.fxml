<AnchorPane id="AnchorPane" prefHeight="420.0" prefWidth="364.0" styleClass="module-background" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="UI.SelectDTD.SelectDTDUIController">
    <stylesheets>
        <URL value="@/UI/UI.css" />
    </stylesheets>
    <children>
      <BorderPane prefHeight="400.0" prefWidth="600.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
         <bottom>
            <HBox alignment="CENTER" maxHeight="50.0" prefHeight="50.0" prefWidth="50.0" spacing="10.0" BorderPane.alignment="CENTER">
